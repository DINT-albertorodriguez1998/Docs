<Window x:Class="Docs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Docs"
        mc:Ignorable="d" Icon="doc_yellow.ico"
        Title="Docs" Height="450" Width="800" SizeToContent="WidthAndHeight" ResizeMode="NoResize">
    <StackPanel>
        <Button x:Name="NuevoDocumentoButton" Click="NuevoDocumentoButton_Click" Margin="5">Nuevo documento</Button>
        <Button x:Name="ConfiguracionButton" Click="ConfiguracionButton_Click" Margin="5">Configuración</Button>
    </StackPanel>
</Window>
